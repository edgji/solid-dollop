{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "db:generate": "zenstack generate",
    "db:push": "dotenvx run -f .dev.vars -- prisma db push",
    "db:studio": "dotenvx run -f .dev.vars -- prisma studio",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc -b",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@prisma/adapter-pg-worker": "^6.3.0",
    "@prisma/client": "6.3.0",
    "@prisma/pg-worker": "^6.3.1",
    "@zenstackhq/runtime": "2.11.5",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20250129.0",
    "@dotenvx/dotenvx": "^1.34.0",
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "prisma": "^6.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "3.107.2",
    "zenstack": "2.11.5"
  },
  "packageManager": "pnpm@8.15.6+sha1.8105075ad0aa306138be128c017e9c41e28ecffa"
}
